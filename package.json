{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "\"Boilerplate for building UI projects with React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "build": "webpack -p",
    "flow": "flow",
    "flow status": "flow status",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "serve": "webpack-dev-server --progress --inline",
    "start": "npm run clean && npm run build && npm run serve",
    "test": "jest --watch --coverage"
  },
  "keywords": [],
  "author": "Trevor Waddell <trevwaddell@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "glamor": "^2.20.40",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "node-sass": "^4.4.0",
    "prettier-eslint": "^6.4.2",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "no-undef": 0,
      "jsx-a11y/href-no-hash": "off",
      "react/jsx-no-bind": 0,
      "no-console": 0,
      "class-methods-use-this": 0,
      "no-return-assign": 0,
      "no-prototype-builtins": 0,
      "react/no-unescaped-entities": 0,
      "react/jsx-filename-extension": 0
    }
  },
  "repository": "https://github.com/trevwaddell/react-starter.git"
}
